{
  "name": "typescript-nuxtjs-boilerplate",
  "private": true,
  "version": "0.1.0",
  "description": "Nuxt.js with TypeScript and Run with docker and docker-compose",
  "scripts": {
    "local": "npm run dev",
    "dev": "cross-env NODE_ENV=development BUILD_ENV=local nuxt-ts",
    "dev:markup": "cross-env NODE_ENV=development BUILD_ENV=local nuxt-ts --spa",
    "build": "nuxt-ts build --no-lock",
    "build:static": "nuxt-ts build --spa --no-lock",
    "build:local": "cross-env NODE_ENV=production BUILD_ENV=local envName=local nuxt-ts build --no-lock",
    "build:docker": "cross-env NODE_ENV=production nuxt-ts build --no-lock",
    "start": "nuxt-ts start",
    "lintjs": "eslint --ext .js,.ts,.vue .",
    "lintjs:fix": "yarn lintjs --fix",
    "lintcss": "stylelint \"src/**/*.{vue,scss}\"",
    "lintcss:fix": "yarn lintcss --fix",
    "textlint": "textlint --config .textlintrc \"./{src,mock,docs}/**/*.md\"",
    "textlint:fix": "yarn textlint --fix",
    "prepush": "yarn lintjs && yarn lintcss && yarn textlint",
    "test": "jest",
    "clean:cache": "rimraf node_modules/.cache/hard-source",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "analyze": "cross-env NODE_ENV=production BUILD_ENV=local nuxt-ts build --analyze",
    "docker": "docker-compose -f docker-compose.yml up --build",
    "docker:clean": "docker-compose -f docker-compose.yml build --no-cache && docker-compose -f docker-compose.yml up",
    "container-prune": "docker container prune",
    "image-prune": "docker image prune",
    "system-prune": "docker system prune",
    "stats": "docker container stats"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  },
  "author": {
    "name": "hisasann",
    "email": "hisasann25@gmail.com",
    "url": "https://github.com/hisasann"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescript-nuxtjs-boilerplate/typescript-nuxtjs-boilerplate.git"
  },
  "keywords": [
    "nuxt",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/typescript-nuxtjs-boilerplate/typescript-nuxtjs-boilerplate/issues"
  },
